{
  "name": "context",
  "version": "0.0.1",
  "description": "test context",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "hollow dev -p 9933",
    "build": "npm run tsc:build && npm run optimize && npm run copy:files",
    "optimize": "npm run babel:cjs & npm run babel:mjs",
    "babel:cjs": "babel dist/lib --out-dir dist/lib --plugins=lodash",
    "babel:mjs": "babel dist/esm --out-dir dist/esm --no-babelrc --plugins=lodash",
    "copy:files": "cp -R style package.json README.md .npmignore dist ",
    "publish2npm": "npm run build && npm publish dist",
    "tsc:build": "rm -rf dist && tsc -p tsconfig.esm.json && cp -R dist/esm dist/lib",
    "start": "jest -o --watch",
    "test": "jest --coverage",
    "codecov": "codecov"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "@types/react-portal": "^4.0.1",
    "@types/react-transition-group": "^2.0.13",
    "babel-jest": "^23.4.2",
    "babel-plugin-import": "^1.8.0",
    "codecov": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "hollow-cli": "^2",
    "jest": "^23.4.2",
    "react": "^16.5.2",
    "react-dom": "^16.4.2",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "publishConfig": {
    "registry": "https://repo.ekuaibao.com/repository/ekuaibao-npm/"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.test.{ts,tsx,js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "/dist/"
    ]
  }
}
